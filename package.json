{
  "dependencies": {
    "@nut-tree/nut-js": "^3.1.2",
    "express": "^4.18.2",
    "ws": "^8.14.2"
  },
  "scripts": {
    "start": "export MODE=DEV && node src/app.js",
    "build": "webpack --mode=development"
  },
  "bin": "src/app.js",
  "name": "anyview",
  "version": "1.0.0",
  "description": "A lightweight remote screen controller directly through web.",
  "main": "src/app.js",
  "keywords": [
    "screenshare",
    "screen"
  ],
  "author": "Sajid Banday",
  "license": "ISC",
  "pkg": {
    "scripts": "public/**/*.js",
    "assets": [
      "public/**/*",
      "node_modules/@nut-tree/**/*.node"
    ],
    "targets": [
      "latest-linux-x64",
      "latest-win-x64"
    ],
    "outputPath": "dist"
  }
}